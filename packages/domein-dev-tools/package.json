{
  "name": "domein-dev-tools",
  "version": "0.5.1",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "2.4.0",
    "@fortawesome/free-brands-svg-icons": "5.12.1",
    "@material-ui/core": "4.9.5",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.45",
    "@material-ui/styles": "4.9.0",
    "@material-ui/system": "4.9.3",
    "@types/react-router-dom": "5.1.3",
    "clsx": "1.1.0",
    "date-fns": "2.10.0",
    "domein": "^0.5.1",
    "domein-react": "^0.5.1",
    "framer-motion": "1.9.0",
    "logrocket": "1.0.6",
    "prismjs": "1.19.0",
    "react": "16.13.0",
    "react-diff-viewer": "3.0.2",
    "react-dom": "16.13.0",
    "react-helmet": "5.2.1",
    "react-intl": "3.12.1",
    "react-json-view": "1.19.1",
    "react-scripts": "3.4.0",
    "react-use": "13.27.0",
    "tiinvo": "1.6.3",
    "typeface-roboto": "0.0.75",
    "yup": "0.28.2"
  },
  "scripts": {
    "start": "SKIP_PREFLIGHT_CHECK=true react-app-rewired start",
    "build": "react-app-rewired build",
    "buildextension": "react-app-rewired build",
    "eject": "react-app-rewired eject",
    "translate:clean": "rimraf ./src/extracted",
    "translate:compile": "tsc -p . --target ES6 --module esnext --jsx preserve --outDir src/extracted --noEmit false",
    "translate:extract": "babel ./src/extracted",
    "translate:manage": "node ./manageTranslations.js",
    "translate:mkdirp": "mkdirp ./src/extracted",
    "translate": "npm run translate:mkdirp && npm run translate:compile && npm run translate:extract && npm run translate:manage && npm run translate:clean",
    "upgrade-interactive": "npm-check -u"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.7",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "@types/chrome": "0.0.98",
    "@types/classnames": "2.2.9",
    "@types/jest": "25.1.3",
    "@types/node": "13.7.7",
    "@types/prismjs": "1.16.0",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-helmet": "5.0.15",
    "@types/react-intl": "3.0.0",
    "@types/yup": "0.26.32",
    "babel-plugin-react-intl": "5.1.18",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "mkdirp": "1.0.3",
    "npm-check": "5.9.2",
    "prettier": "1.19.1",
    "react-app-rewired": "2.1.5",
    "react-intl-translations-manager": "5.0.3",
    "rimraf": "3.0.2",
    "typescript": "3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
