{
  "name": "domein-react",
  "version": "0.5.1",
  "licence": "MIT",
  "author": {
    "name": "Paolo Roth",
    "email": "iamoctod@gmail.com"
  },
  "main": "dist/index.js",
  "description": "domein react bindings",
  "keywords": [
    "typescript"
  ],
  "scripts": {
    "changelog": "standard-changelog",
    "clean": "rimraf dist",
    "docs": "npm run clean && typedoc --options typedoc.markdown.json",
    "lint": "eslint -c ./.eslintrc.json src/**/*.ts --fix",
    "prepublishOnly": "npm run clean && tsc -p tsconfig.npm.json",
    "size-limit": "npm run clean && tsc -p . && size-limit",
    "test": "jest",
    "upgrade-interactive": "npm-check --update"
  },
  "homepage": "https://github.com/yourname/yourrepo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourname/yourrepo.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "size-limit": [
    {
      "limit": "1 s",
      "webpack": false,
      "path": "dist/*.js"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "4.0.1",
    "@size-limit/time": "4.4.2",
    "@testing-library/react": "9.5.0",
    "@types/benchmark": "1.0.31",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.5",
    "@types/react": "16.9.26",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "benchmark": "2.1.4",
    "codecov": "3.6.5",
    "domein": "^0.5.1",
    "eslint": "6.8.0",
    "husky": "4.2.3",
    "jest": "25.2.3",
    "lint-staged": "10.0.9",
    "npm-check": "5.9.2",
    "prettier": "1.19.1",
    "react-dom": "16.13.1",
    "rimraf": "3.0.2",
    "standard-changelog": "2.0.21",
    "ts-jest": "25.2.1",
    "ts-node": "8.8.1",
    "typedoc": "0.17.3",
    "typedoc-plugin-markdown": "2.2.17",
    "typescript": "3.7.5"
  },
  "peerDependencies": {
    "react": "^16.12.0"
  }
}
